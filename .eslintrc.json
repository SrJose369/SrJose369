{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "globals": {
        "chrome": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes": "off",
        "prefer-arrow-callback": "off",
        "indent": ["error", 4],
        "no-console": "off",
        "linebreak-style": ["warn", "unix"],
        "prefer-template": "off",
        "func-names": "off",
        "object-curly-spacing": "off",
        "space-infix-ops": "off",
        "camelcase": "off",
        "spaced-comment": 0,
        "no-param-reassign": "warn",
        "prefer-const": "warn",
        "no-var": "off",
        "no-unused-vars": "warn",
        "no-use-before-define": 0,
        "vars-on-top": "warn",
        "one-var": "warn",
        "keyword-spacing": "warn",
        "no-lonely-if": 0,
        "prefer-destructuring": 0,
        "no-multi-spaces": "warn",
        "comma-spacing": "warn",
        "no-plusplus": "warn",
        "max-len": 0,
        "no-trailing-spaces": "warn",
        "no-confusing-arrow": 0,
        "no-multi-assign": 0
        
    }
}
